<?xml version="1.0" encoding="UTF-8" ?>
<construct>
    <construct_parameters>
        <param name="verbose initialization" value="true" />
        <param name="verbose runtime" value="true" />
    </construct_parameters>
    <models>
        <model name="Stance Model" />
    </models>
    <outputs>
        <output type="dynetml">
            <param name="network names" value="stance network, influence network" />
            <param name="output file" value="my_output.xml" />
            <param name="timeperiods" value="all" />
        </output>
		<output type="file">
			
		</output>
    </outputs>
    <nodesets>
        <nodeset name="agent">
			<generator type="dynetml">
				<param name="file name" value="TwitterNetworkFromORA.xml"/>
				<param name="nodeset name" value="Agent"/>
			</generator>
        </nodeset>
		<nodeset name="stance">
			<generator type="constant">
				<count value="1" />
			</generator>
		</nodeset>
        <nodeset name="time">
            <generator type="constant">
                <count value="10" />
            </generator>
        </nodeset>
    </nodesets>
    <networks>
        <network name="stance network" edge_type="float" default="0">
            <source nodeset="agent" representation="dense" />
            <target nodeset="stance" representation="dense" />
        </network>
        <network name="influence network" edge_type="float" default="0">
			<generator type="dynetml">
				<param name="file" value="TwitterNetworkFromORA.xml"/>
				<param name="network name" value="Agent x Agent - All Communication"/>
			</generator>
            <source nodeset="agent" representation="dense" />
            <target nodeset="agent" representation="dense" />
        </network>
    </networks>
</construct>
